local API = require (game. ServerScriptService:WaitforChild("IrelloAPI*))
local BanBoardID - API:GetBoardID ("Test Bans")
local BaniistID - API:GetlistID ("UserIDBans", BanBoardID)

function checkurrentPlayersForBan()
    while wait (4) do
        local BanCards = API:GetCardsInlist(BanlistID)
        for _,Player in pairs(game.Players:GetChildren()) do
For
Card in pairs (BanCards) do
Tr string. find (Card.name, Player.UgerId)
then
Plaver:Kick ("You have been banned.")
end
end
end
end
end
